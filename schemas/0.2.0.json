          "default": 10,
          "exclusiveMinimum": 0,
          "minimum": 1,
          "title": "Max Generations",
          "type": "integer"
        },
        "population_size": {
          "default": 10,
          "exclusiveMinimum": 0,
          "maximum": 200,
          "minimum": 2,
          "title": "Population Size",
          "type": "integer"
        },
        "max_stall_generations": {
          "default": 10,
          "exclusiveMinimum": 0,
          "minimum": 1,
          "title": "Max Stall Generations",
          "type": "integer"
        },
        "worker_count": {
          "default": 4,
          "exclusiveMinimum": 0,
          "minimum": 1,
          "title": "Worker Count",
          "type": "integer"
        },
        "linear_inequalities": {
          "anyOf": [
            {
              "$ref": "#/$defs/LinearInequalities"
            },
            {
              "type": "null"
            }
          ],
          "default": null
