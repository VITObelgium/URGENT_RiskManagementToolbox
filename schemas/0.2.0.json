    "HWellModel": {
      "additionalProperties": false,
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "wellhead": {
          "$ref": "#/$defs/PositionModel"
        },
        "md_step": {
          "default": 0.5,
          "minimum": 0.1,
          "title": "Md Step",
          "type": "number"
        },
        "perforations": {
          "anyOf": [
            {
              "additionalProperties": {
                "$ref": "#/$defs/PerforationRangeModel"
              },
              "propertyNames": {
                "$ref": "#/$defs/PerforationName"
              },
              "type": "object"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Perforations"
        },
        "well_type": {
          "const": "HWell",
          "default": "HWell",
          "title": "Well Type",
          "type": "string"
        },
        "TVD": {
          "exclusiveMinimum": 0.0,
          "title": "Tvd",
          "type": "number"
        },
        "md_lateral": {
          "exclusiveMinimum": 0.0,
          "title": "Md Lateral",
          "type": "number"
        },
        "azimuth": {
          "exclusiveMaximum": 360.0,
          "minimum": 0.0,
          "title": "Azimuth",
          "type": "number"
        }
      },
      "required": [
        "name",
        "wellhead",
        "TVD",
        "md_lateral",
        "azimuth"
      ],
      "title": "HWellModel",
      "type": "object"
    },
